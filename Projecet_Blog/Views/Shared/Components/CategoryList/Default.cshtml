@model IEnumerable<Entity_Layer.Concrete.Category>
<div class="tech-btm">
    <h4>kategoriler</h4>
    <ul class="list-group single">
        @foreach (var item in Model)
        {
            <li class="list-group-item d-flex justify-content-between align-items-center">
                @item.CategoryName

                <span class="badge badge-primarybadge-pill">0</span>
            </li>

        }

    </ul>
</div>